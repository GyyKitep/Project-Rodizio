package br.com.rodizio.module;

import java.sql.Timestamp;
import java.util.List;

import jakarta.persistence.Entity;
import jakarta.persistence.JoinColumn;
import jakarta.persistence.ManyToMany;
import jakarta.persistence.Table;

@Entity
@Table(name = "establishment_categories")
public class Establishment_Categories {
	

    @ManyToMany(mappedBy = "establishment")
    @JoinColumn(name = "establishment_id")
    private List<Establishment> establishment_id;
    
    @ManyToMany(mappedBy = "categorias")
    @JoinColumn(name = "categories_id")
    private List<Categories> categories_id;        
	private Timestamp created_at;
	
	

}
